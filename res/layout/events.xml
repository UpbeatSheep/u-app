<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
    <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout1" android:baselineAligned="true" android:layout_height="wrap_content" android:paddingLeft="10dip" android:paddingTop="10dip">
        <Button android:id="@+id/eventmon" android:maxWidth="50dip" android:background="@drawable/eventmon" android:maxHeight="50dip" android:layout_width="100dip" android:layout_height="100dip"></Button>
        <Button android:maxWidth="50dip" android:id="@+id/eventtue" android:background="@drawable/eventtue" android:maxHeight="50dip" android:layout_width="100dip" android:layout_height="100dip"></Button>
        <Button android:maxWidth="50dip" android:id="@+id/eventwed" android:background="@drawable/eventwed" android:maxHeight="50dip" android:layout_width="100dip" android:layout_height="100dip"></Button>
    </LinearLayout>
    <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout2" android:layout_height="wrap_content" android:paddingLeft="10dip">
        <Button android:text="Button" android:id="@+id/eventthur" android:maxWidth="50dip" android:maxHeight="50dip" android:background="@drawable/eventthur" android:layout_width="100dip" android:layout_height="100dip"></Button>
        <Button android:text="Button" android:id="@+id/eventfri" android:maxWidth="50dip" android:maxHeight="50dip" android:background="@drawable/eventfri" android:layout_width="100dip" android:layout_height="100dip"></Button>
        <Button android:text="Button" android:id="@+id/eventsat" android:maxWidth="50dip" android:maxHeight="50dip" android:background="@drawable/eventsat" android:layout_width="100dip" android:layout_height="100dip"></Button>
       </LinearLayout>
    <ListView android:dividerHeight="0dip" android:id="@+id/android:list" android:layout_height="wrap_content" android:layout_width="match_parent" android:cacheColorHint="#00000000" android:layout_weight="1"></ListView>
</LinearLayout>