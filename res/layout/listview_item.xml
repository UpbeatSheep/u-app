<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<ImageView android:layout_alignParentTop="true" android:id="@+id/image" android:layout_height="75dip" android:layout_width="fill_parent" android:scaleType="centerCrop" android:src="@drawable/initial" android:layout_marginBottom="@dimen/tile_margin"></ImageView>
	<TextView android:layout_height="wrap_content" android:text="TextView" android:layout_width="wrap_content" android:id="@+id/text" style="@style/Tile" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"></TextView>
</RelativeLayout>
